from collections import Counter 

class Solution:
    def checkEqual(self, a, b) -> bool:
        #code here
         
        if len(a) != len(b):
            return False
        
        return Counter(a) == Counter(b)    
        
        # return a.sort() == b.sort()    
            
        # for i in a:
        #     if i in b:
        #         return True 
        
        # # for i in range(len(a)):
        # #     if a[i] != b[i]:
        # #         return False
                
        # return False
