class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        prevMap = defaultdict()
        for i, n in enumerate(nums):
            diff = target - n
            if diff in prevMap:
                return [prevMap[diff], i]
            prevMap[n] = i

        # res = []
        # n = len(nums)
        # i = 0 
        # for j in range(1,n):
        #     if nums[i] + nums[j] == target:
        #         return [i,j]
        #         i += 1   

        # return res            

