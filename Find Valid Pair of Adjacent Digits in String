from collections import Counter

class Solution(object):
    def findValidPair(self, s):
        """
        :type s: str
        :rtype: str
        """
        counts = Counter(s)
        
        for i in range(len(s) - 1):
            char1 = s[i]
            char2 = s[i+1]
            
            if char1 != char2:
                if counts[char1] == int(char1) and counts[char2] == int(char2):
                    return char1 + char2
        return ""
