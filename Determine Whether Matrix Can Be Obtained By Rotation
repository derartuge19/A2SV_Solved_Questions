n = len(matrix)

        for _ in range(4):
             
             
            for i in range(n):
                for j in range(i + 1,n):
                    matrix[i][j], matrix[j][i] = matrix[j][i], matrix[i][j]
        
            for i in range(n):
                matrix[i].reverse()

            if mat == target:
                return True 

        return False
