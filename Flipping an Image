class Solution(object):
    def flipAndInvertImage(self, image):
        """
        :type image: List[List[int]]
        :rtype: List[List[int]]
        """
        n = len(image)
        for i in range(n):
            image[i].reverse()

        rows = len(image)
        cols = len(image[0])

        for i in range(rows):
            for j in range(cols):
                image[j][i] = 1 - image[j][i]    

        # for i, row in enumerate(image):
        #     for j, value in enumerate(row):
        #         image[i][j] = 1 - value      
                    

        return image                
        
