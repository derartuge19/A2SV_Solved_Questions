from collections import Counter

class Solution(object):
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        c1 = Counter(s)
        c2 = Counter(t)

        return c1 == c2


        # if len(t) != len(s):
        #     return False 

        # for i in s:
        #     if i in t:
        #         return True  

        # return False            
        
