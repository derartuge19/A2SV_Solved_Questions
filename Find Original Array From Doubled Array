from collections import Counter

class Solution(object):
    def findOriginalArray(self, changed):
        """
        :type changed: List[int]
        :rtype: List[int]
        """
        if len(changed) % 2 != 0:
            return []
        
        count = Counter(changed)
        changed.sort()
        
        original = []
        
        for x in changed:
            
            if count[x] == 0:
                continue
            
            count[x] -= 1
            
            double = x * 2
            if count[double] > 0:
                count[double] -= 1
                original.append(x)
            else:
                return []
                
        return original
